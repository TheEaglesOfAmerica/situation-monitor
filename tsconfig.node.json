{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noEmit": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"include": ["*.config.ts", "*.config.js", "playwright.config.ts"]
}
